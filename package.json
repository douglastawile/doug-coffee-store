{
  "name": "doug-coffee-store",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "run-p dev start-api",
    "dev": "cross-env REACT_APP_API_BASE_URL=http://localhost:3001/ vite",
